        <put-list-attribute name="jsUrls">
            <add-attribute value="${jqueryui.url}" />
            <add-attribute value="${jquery.plugins}/jquery.jeditable.mini.js" />
            <add-attribute value="${jquery.plugins}/jquery.maskedinput-1.3.min.js" />
